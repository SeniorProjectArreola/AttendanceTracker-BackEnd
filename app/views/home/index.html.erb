<div id="wrapper">
  <nav class="navbar navbar-default navbar-fixed-top navbar-global" role="navigation">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#"><%= image_tag("logo-lg.png", :alt => "attendance tracker") %></a>
    </div>
    <div class="collapse navbar-collapse navbar-ex1-collapse">
      <ul class="nav navbar-nav side-nav">
        <li class="list-label">My Events:</li>
        <% @admin_events.each do |event| %>
        <li class="active"><%= link_to event.name, event %></li>
        <% end %>
        <% if false %>
        <li class="list-label-b">Event's I am Attending:</li>
        <% @client_events.each do |event| %>
        <li class="active"><%= link_to event.name, event %></li>
        <% end %>
        <% end %>
      </ul>
      <ul class="nav navbar-nav navbar-right navbar-user">
        <li class="dropdown">
          <a href="#" class="dropdown-toggle user-dropdown" data-toggle="dropdown"><%= current_user.email %><b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li><%= link_to 'Logout', destroy_user_session_path, :method => :delete %></li>
          </ul>
        </li>
      </ul>
    </div><!-- /.navbar-collapse -->
  </nav>
  <div id="page-wrapper">
    <div class="row">
    <div class="col-sm-12 main">
    <div class="row cont-row-b">
      <h3 class="sub-header">Welcome!</h3>
      <div class="col-xs-12 home-h1">What would you like to do?</div>
      <div class="col-xs-12 new-btns">
        <div class="cont-row-c">
          <%= image_tag("list-icon.png", :alt => "list icon") %>
          <%= link_to 'New Event', new_event_path %>
        </div>
        <div class="cont-row-c">
          <%= image_tag("list-icon.png", :alt => "list icon") %>
          <%= link_to 'View All Events', events_path %>
        </div>
      </div>
    </div>
  </div>
  </div>
  </div><!-- /.row -->
</div><!-- /#page-wrapper -->
</div><!-- /#wrapper -->